<?xml version="1.0" encoding="UTF-8"?>
<node>
	<interface name="com.lomiri.LomiriAppLaunch">
		<signal name="LomiriResumeRequest">
			<arg type="s" name="appid" />
			<arg type="s" name="instance" />
		</signal>
		<signal name="LomiriResumeResponse">
			<arg type="s" name="appid" />
			<arg type="s" name="instance" />
		</signal>
		<signal name="LomiriFocusRequest">
			<arg type="s" name="appid" />
			<arg type="s" name="instance" />
		</signal>
		<signal name="LomiriStartingBroadcast">
			<arg type="s" name="appid" />
			<arg type="s" name="instance" />
		</signal>
		<signal name="LomiriStartingSignal">
			<arg type="s" name="appid" />
			<arg type="s" name="instance" />
		</signal>
		<signal name="ApplicationFailed">
			<arg type="s" name="appid" />
			<arg type="s" name="instance" />
			<arg type="s" name="stage" />
		</signal>
		<signal name="ApplicationPaused">
			<arg type="s" name="appid" />
			<arg type="s" name="instance" />
			<arg type="at" name="pids" />
		</signal>
		<signal name="ApplicationResumed">
			<arg type="s" name="appid" />
			<arg type="s" name="instance" />
			<arg type="at" name="pids" />
		</signal>
	</interface>
</node>
